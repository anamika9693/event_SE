    <script>

        // REQUEST A DEMO - iframe with cookies
        function setCookie(cname, cvalue, exdays) {
            const d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            let expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }

        function getCookie(cname) {
            let name = cname + "=";
            let ca = document.cookie.split(';');
            for (let i = 0; i < ca.length; i++) {
                let c = ca[i];
                while (c.charAt(0) == ' ') {
                    c = c.substring(1);
                }
                if (c.indexOf(name) == 0) {
                    return c.substring(name.length, c.length);
                }
            }
            return "";
        }




        function setSourceTrackingCookie() {
            let query = window.location.search.substring(1);
            setCookie("sourcetracking", query, 30);
        }


        function createRequestADemoIframe() {

            let sourcetracking = getCookie("sourcetracking");

            let iframe =
                `<div 
		class="meetings-iframe-container" data-src="https://meetings.hubspot.com/dani-morio/sdr-request-a-demo?embed=true">
	</div>`
                ;

            let sourceString = "";
            if (sourcetracking != "") {
                iframe =
                    `<div 
			class="meetings-iframe-container" data-src="https://meetings.hubspot.com/dani-morio/sdr-request-a-demo?embed=true&${sourcetracking}">
		</div>`
                    ;

            }


            return iframe;


        }



        //get url paramteres, if UTM parameters exist, set sourcetracking cookie
        let searchParams = new URLSearchParams(window.location.search)
        if (searchParams.has('utm_campaign') && searchParams.has('utm_source') && searchParams.has('utm_medium')) {
            setSourceTrackingCookie();
        }


        if ($("#request-a-demo-iframe").length > 0) {
            $("#request-a-demo-iframe").append(createRequestADemoIframe());
        }


        if ($(".request-a-demo-iframe").length > 0) {
            $(".request-a-demo-iframe").each(function () {
                $(this).append(createRequestADemoIframe());
            })

        }



    </script>

